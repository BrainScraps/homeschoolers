<h1>Gettogether#index</h1>
<p>Find me in app/views/gettogether/index.html.erb</p>

<table class="table"> 
  <thead> 
<% @gtgs.each do |gtg| %>
  <tr><%=  link_to gtg.name, gettogether_path(gtg.id) %> </li>
</table> 

<% end %>


<%= weekly_calendar(@events, :date => @date, :include_24_hours => true) do |w| %>
    <% binding.pry %>
  <%= w.week(:business_hours => params[:business_hours], :clickable_hours => true) do |event,truncate| %>

    <%=  event.start_time.strftime('%I:%M%p') %>
    <%=  link_to truncate(event.name,truncate), '#' %>
    <% end %>
  <% end %>

<p> <%= link_to "New Get-Together", new_gettogether_path %> 